      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": {
        "type": "TextualBody",
        "value": "Es una lucha que dura años, que cuesta mucho. Miles de compañeros durante decenas de años pagaron con su vida la prima y los primeros plazos por nuestra libertad.",
        "format": "text/plain",
        "purpose": "commenting"
      },
      "target": {
        "source": "https://lgsump.github.io/radio-venceremos-espanol/1981-05-27-anotaciones-parte-1/canvas-1/canvas",
        "selector": {
          "type": "RangeSelector",
