    {
      "@context": "http://www.w3.org/ns/anno.jsonld",
      "id": "1981-05-27-anotaciones-parte-1-canvas-1-m-sica-annotation-10.json",
      "type": "Annotation",
      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": {
        "type": "TextualBody",
        "value": "Daremos nuestra vida por la patria. Porque la libertad aguantaremos. La patria en esta tierra venceremos. Trabajando día a día viviremos. Tiranos y asesinos ya no más.",
        "format": "text/plain",
        "purpose": "commenting"
      },
      "target": {
        "source": "https://lgsump.github.io/radio-venceremos-espanol/1981-05-27-anotaciones-parte-1/canvas-1/canvas",
