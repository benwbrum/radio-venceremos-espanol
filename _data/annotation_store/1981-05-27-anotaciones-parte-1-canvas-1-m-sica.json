    {
      "@context": "http://www.w3.org/ns/anno.jsonld",
      "id": "1981-05-27-anotaciones-parte-1-canvas-1-m-sica-annotation-11.json",
      "type": "Annotation",
      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": {
        "type": "TextualBody",
        "value": "Ven salvadoreño, vamos a luchar, la patria espera por tu libertad, por tu libertad, por tu libertad.",
        "format": "text/plain",
        "purpose": "commenting"
      },
      "target": {
        "source": "https://lgsump.github.io/radio-venceremos-espanol/1981-05-27-anotaciones-parte-1/canvas-1/canvas",
